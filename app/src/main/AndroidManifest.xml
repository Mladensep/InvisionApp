<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.invisionapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/invision"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.activity.activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.activity.activity.HomeActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity.activity.activity.ContactActivity"
            android:windowSoftInputMode="stateHidden"></activity>
    </application>

</manifest>